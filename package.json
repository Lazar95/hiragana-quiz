{
  "name": "hiragana-quiz",
  "version": "1.0.0",
  "description": "A simple flashcard-like quiz developed in Angular for testing knowledge of Japanese writing script hiragana ",
  "main": "index.js",
  "scripts": {
    "start": "npm run tsc && npm run sass && concurrently \"npm run tsc:w\" \"npm run sass:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass": "node-sass app/ -o app/",
    "sass:w": "node-sass -w app/ -o app/",
    "typings": "typings",
    "test": "jasmine"
  },
  "keywords": [
    "japanese",
    "hiragana",
    "quiz",
    "flashcards",
    "angular2",
    "typescript"
  ],
  "author": "Lazar LjubenoviÄ‡",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "angular2-in-memory-web-api": "0.0.15",
    "core-js": "^2.4.0",
    "lodash": "^4.14.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "material-colors": "^1.0.0",
    "material-shadows": "^3.0.0",
    "node-sass": "^3.8.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
